{
    "name"        : "alirezadp10/seeder",
    "description" : "Improving seeding functionality of Laravel framework.",
    "author"      : "alirezadp10",
    "keywords"    : [
        "laravel",
        "database",
        "seeder"
    ],
    "license"     : "MIT",
    "authors"     : [
        {
            "name": "Alireza Goodarzi"
        }
    ],
    "require-dev" : {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit"    : "^9.3"
    },
    "require"     : {
        "php"                  : ">=7.1",
        "illuminate/support"   : "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "autoload"    : {
        "psr-4": {
            "Seeder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seeder\\Tests\\": "tests/"
        }
    },
    "extra"       : {
        "laravel": {
            "providers": [
                "Seeder\\SeederServiceProvider"
            ]
        }
    }
}
